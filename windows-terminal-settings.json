{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
        {
            "command": {
                "action": "copy",
                "singleLine": true
            },
            "keys": "ctrl+shift+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+shift+v"
        },
        {
            "command": "closeTab",
            "keys": "ctrl+f4"
        },
        {
            "command": "newTab",
            "keys": "ctrl+t"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "ctrl+shift+s"
        },
        {
            "command": "closePane",
            "keys": "ctrl+shift+f4"
        },
        {
            "command": "find",
            "keys": "ctrl+f"
        },
        {
            "command": {
                "action": "findMatch",
                "direction": "next"
            },
            "keys": "shift+f3"
        },
        {
            "command": {
                "action": "findMatch",
                "direction": "prev"
            },
            "keys": "f3"
        },
        {
            "command": "scrollUpPage",
            "keys": "pageup"
        },
        {
            "command": "scrollDownPage",
            "keys": "pagedown"
        },
        {
            "command": "scrollToTop",
            "keys": "ctrl+home"
        },
        {
            "command": "scrollToBottom",
            "keys": "ctrl+end"
        },
        {
            "command": {
                "action": "openSettings",
                "target": "settingsUI"
            },
            "keys": "ctrl+shift+,"
        }
    ],
    "copyFormatting": false,
    "copyOnSelect": false,
    "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
    "newTabMenu": [
